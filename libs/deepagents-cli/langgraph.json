{
  "dependencies": ["."],
  "graphs": {
    "agent": "./deepagents_cli/graph.py:graph"
  },
  "env": ".env",
  "checkpointer": {
    "ttl": {
      "strategy": "delete",
      "sweep_interval_minutes": 120,
      "default_ttl": 20160
    }
  },
  "store": {
    "ttl": {
      "refresh_on_read": true,
      "sweep_interval_minutes": 120,
      "default_ttl": 20160
    }
  }
}
